    "@types/express": "^4.17.21",
    "@types/http-errors": "^2.0.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.2",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.15.0",
